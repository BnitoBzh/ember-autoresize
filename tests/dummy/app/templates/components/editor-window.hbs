<div class="window_chrome">
  <span class="close button"></span>
  <span class="minimize button"></span>
  <span class="maximize button"></span>
  {{title}}
</div>
<div class="window_content">
  <ul class="gutter">
    {{#each lines as |line|}}
      <li>{{line.number}}</li>
    {{/each}}
  </ul>
  <code>
    {{~#each lines as |line|~}}
      {{line.code}}
    {{~/each~}}
  </code>
</div>
